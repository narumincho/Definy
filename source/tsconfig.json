{
  "compilerOptions": {
    "target": "ES2018",
    "lib": ["DOM"],
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "skipLibCheck": true
  }
}
