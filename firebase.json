{
    "hosting": {
        "public": "client/distribution",
        "headers": [
            {
                "source": "main.js",
                "headers": [
                    {
                        "key": "cache-control",
                        "value": "max-age=31536000"
                    }
                ]
            }
        ],
        "rewrites": [
            {
                "source": "sitemap",
                "function": "sitemap"
            },
            {
                "source": "**",
                "function": "indexHtml"
            }
        ]
    },
    "functions": {
        "source": "server/distribution"
    }
}
