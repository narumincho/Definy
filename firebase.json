{
  "hosting": {
    "public": "distribution",
    "headers": [
      {
        "source": "icon",
        "headers": [
          {
            "key": "content-type",
            "value": "image/png"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "sitemap",
        "function": "sitemap"
      },
      {
        "source": "**",
        "function": "indexHtml"
      }
    ]
  }
}
